<br/><%= make_breadcrumb %>   
 <%= render_breadcrumbs  %>
<span style="float:right;"><%= link_to "logout", logout_path %></span>
<% if permitted_to? :index %><br/>
<h3 class ='mt-5'>Creating Slots For Doctors</h3>
<table class = "table table-hover">
    <thead>
  <tr>
    <th scope = "col">Name</th>
    <th scope ="col">Action</th>
  </tr>
    </thead>
    <tbody>
<% for doctor in @doctors %>
  <tr>
    <td><%=h doctor.first_name%></td>
    <td><%= link_to 'Slot',total_slots_slot_path(doctor.id)%></td>
