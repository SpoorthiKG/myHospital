<%if @histroys.present?%>
<h3 class='mt-5'>Listing  Previous Records</h3>
<table class = "table table-hover">
    <thead>
  <tr>
    <th scope = "col">Date of Admission</th>
    <th scope = "col">Date of Discharge</th>
    <th scope='col'>Bed</th>
      </tr>
    </thead>
    <tbody>
<% @histroys.each do |discharged_patient| %>
  <tr>
    <td><%= discharged_patient.admitted_on %></td>
    <td><%= discharged_patient.discharged_on %></td>
    <td ><%= discharged_patient.bed_id%></td>
  </tr>
<% end %>
  <%else%>
 <br/> <h2 class='mt-5' style = "color:red; text-align:center">No Previous Histroy</h2>
  <%end%>
  </tbody>
</table>
